{
    "apps": [
        {
            "name": "omniaccess-web",
            "script": "npm",
            "args": "start",
            "cwd": "./",
            "instances": 1,
            "exec_mode": "fork",
            "autorestart": true,
            "watch": false,
            "max_memory_restart": "1G",
            "env": {
                "NODE_ENV": "production",
                "PORT": "10001"
            },
            "error_file": "./logs/web-error.log",
            "out_file": "./logs/web-out.log",
            "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
            "merge_logs": true
        },
        {
            "name": "omniaccess-webhooks",
            "script": "server.js",
            "cwd": "./",
            "instances": 1,
            "exec_mode": "fork",
            "autorestart": true,
            "watch": false,
            "max_memory_restart": "512M",
            "env": {
                "NODE_ENV": "production",
                "WEBHOOK_PORT": "10000",
                "HOST": "0.0.0.0"
            },
            "error_file": "./logs/webhooks-error.log",
            "out_file": "./logs/webhooks-out.log",
            "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
            "merge_logs": true
        }
    ]
}